<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="DMES_PersistenceUnit" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.mycompany.dmes.entitys.ScMails</class>
    <class>com.mycompany.dmes.entitys.ScPersonDocumentationAttached</class>
    <class>com.mycompany.dmes.entitys.ScPersonSpecifications</class>
    <class>com.mycompany.dmes.entitys.ScPhones</class>
    <class>com.mycompany.dmes.entitys.ScModulePermission</class>
    <class>com.mycompany.dmes.entitys.ScModulePermissionByRole</class>
    <class>com.mycompany.dmes.entitys.ScPerson</class>
    <class>com.mycompany.dmes.entitys.ScPersonObservations</class>
    <class>com.mycompany.dmes.entitys.ScRoles</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/DMES"/>
      <property name="javax.persistence.jdbc.password" value="sipPrueba"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.user" value="sipPrueba"/>
      <property name="eclipselink.ddl-generation" value="create-tables"/>
    </properties>
  </persistence-unit>
</persistence>
